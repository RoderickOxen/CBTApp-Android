<selector xmlns:android="http://schemas.android.com/apk/res/android">


    <item
        android:state_pressed="true">
        <shape>
            <solid
                android:color="@color/blueTec2"/>
            <stroke
                android:color="@color/blueTec1"
                android:width="1dp"/>
        </shape>
    </item>


    <item>
        <shape>
            <solid
                android:color="@color/blueTec3"/>
            <stroke
                android:color="@color/colorPrimaryDark"
                android:width="1dp"/>
        </shape>
    </item>

</selector>